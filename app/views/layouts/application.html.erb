<!DOCTYPE html>
<html>
<head>
  <title>Dosomething</title>

  <%= stylesheet_link_tag    'http://fezvrasta.github.io/bootstrap-material-design/dist/css/bootstrap-material-design.css' %>
  <%= stylesheet_link_tag    'http://fezvrasta.github.io/bootstrap-material-design/dist/css/ripples.min.css' %>
  <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/animate.css/3.2.0/animate.min.css">
  <%= stylesheet_link_tag    'https://fonts.googleapis.com/icon?family=Material+Icons' %>
  <%= stylesheet_link_tag    'http://fonts.googleapis.com/css?family=Roboto:300,400,500,700' %>
  <%= stylesheet_link_tag    'application' , 'data-turbolinks-track' => true %>

  <%= javascript_include_tag 'application' , 'data-turbolinks-track' => true %>

  <%= csrf_meta_tags %>

</head>

<body>



<% if user_signed_in? %>

        <div class="item-right">
          <%= link_to '<i style="font-size:20px" class="fa fa-power-off"></i>'.html_safe, destroy_user_session_path, method: :delete, class: "btn btn-primary " %>
        </div>

<% end %>

<%= yield %>

<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>

</body>
<script>
  $(function () {
    $.material.init();
  });
</script>
<%= javascript_include_tag 'http://fezvrasta.github.io/bootstrap-material-design/dist/js/material.min.js'  %>
<%= javascript_include_tag 'http://fezvrasta.github.io/bootstrap-material-design/dist/js/ripples.min.js'  %>

</html>
